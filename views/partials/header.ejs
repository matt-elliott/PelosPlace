<header id="site-header">
  <nav class="navbar">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#site-menu" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>

        <div class="navbar-brand">
          <h1>
            <a href="#"><%- data.siteData.site_name %></a>
          </h1>
          <h3><%- data.siteData.site_tagline %></h3>
        </div>
      </div>

      <div class="collapse navbar-collapse" id="site-menu">
        <ul class="nav navbar-nav" id="social-links">
          <% for(var key in data.socialNetworkLinks) { %>
            <% var cleanName = key.split('_')[2]; %>
            <li><a href="<%- data.socialNetworkLinks[key] %>" class="icon-<%- cleanName %>" title="<%- cleanName %>" target="_blank"></a></li>
          <% }; %>
        </ul>
        <ul class="nav navbar-nav" id="page-links">

          <% data.modules.forEach(function(module) { %>
            <li>
              <a ng-href="#<%- module %>" target="_self"><%- module %></a>
            </li>
          <% }) %>
          
        </ul>
      </div>
    </div>
  </nav>
</header>